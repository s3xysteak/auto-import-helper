{
  "name": "auto-import-helper",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "description": "Help you create projects that support `unplugin-auto-import`!",
  "author": "s3xysteak",
  "license": "MIT",
  "homepage": "https://github.com/s3xysteak/auto-import-helper",
  "repository": {
    "type": "git",
    "url": "https://github.com/s3xysteak/auto-import-helper.git"
  },
  "bugs": "https://github.com/s3xysteak/auto-import-helper/issues",
  "keywords": [
    "unplugin-auto-import",
    "utils"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "type:check": "tsc",
    "release": "bumpp",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/node": "^22.7.8",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "esno": "^4.8.0",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9"
      }
    }
  }
}
